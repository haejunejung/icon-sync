{
	"name": "icon-sync",
	"version": "0.0.0",
	"description": "Sync icons from Figma to GitHub",
	"keywords": ["icons", "iconSync", "figma plugin"],
	"packageManager": "pnpm@10.10.0",
	"author": "haejunejung",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"watch:ui": "vite build --watch",
		"watch:code": "nodemon --watch src --ext ts,tsx,cjs --exec 'node build.cjs'",
		"build": "tsc -b & vite build & node build.cjs",
		"build:watch": "npm-run-all --parallel watch:ui watch:code",
		"format": "biome format --write",
		"lint": "biome lint --write",
		"check": "biome check --write",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"publish-packages": "changeset publish"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@changesets/cli": "^2.29.2",
		"@figma/plugin-typings": "^1.110.0",
		"@octokit/types": "^14.0.0",
		"@swc/cli": "^0.7.3",
		"@swc/core": "^1.11.24",
		"@swc/helpers": "^0.5.17",
		"@types/node": "^22.15.3",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.3",
		"@vitejs/plugin-react": "^4.4.1",
		"esbuild": "^0.25.3",
		"nodemon": "^3.1.10",
		"npm-run-all": "^4.1.5",
		"tw-animate-css": "^1.2.8",
		"typescript": "^5.8.3",
		"vite-plugin-singlefile": "^2.2.0"
	},
	"dependencies": {
		"@create-figma-plugin/utilities": "^4.0.1",
		"@octokit/rest": "^21.1.1",
		"@radix-ui/react-label": "^2.1.4",
		"@radix-ui/react-slot": "^1.2.0",
		"@radix-ui/react-tabs": "^1.1.9",
		"@tailwindcss/vite": "^4.1.5",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router": "^7.5.3",
		"tailwind-merge": "^3.2.0",
		"tailwindcss": "^4.1.5",
		"vite": "^6.3.4"
	},
	"publishConfig": {
		"access": "public"
	}
}
